#include <iostream>
#include <cmath>
using namespace std;


void calc(int x, int y, int z){ //функция calc
    float x1,x2; //объявление переменных, значение которых - нецелые числа
    float D = y*y - 4*x*z; //объявление переменной дискриминанта
    if (D>0) {
        x1 = (-y + sqrt(D))/2*x;
        x2 = (-y - sqrt(D))/2*x;
        cout <<"Korni -> " <<x1 <<" "<< x2; //при D>0, выполняется нахождение корней х1 и х2, их вывод
		 }
	  else if (D==0) {
        x1 = -y/2*x;
        cout <<"Koren -> " << x1 ; //при D=0, выполняется нахождение х1, его вывод

     } else {
        cout<<"Korney net"; //в случае D<0, выводится "корней нет"
     }

}


int main(){
	cout<<"Type x, y and z"<<endl;
    int x,y,z; //объявление переменных
    cin >> x>> y >> z; //ввод значений переменных
    calc(x,y,z); //обращение к функции
 


}

